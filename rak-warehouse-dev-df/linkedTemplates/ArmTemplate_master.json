{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rak-warehouse-dev-df"
		},
		"Lnk_Service_AzSQL_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Lnk_Service_AzSQL_DB'"
		},
		"AKV_Test_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultlab1.vault.azure.net/"
		},
		"LnkSR_File_Server_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\DevOps\\Self-Hosted-Integration-RunTime\\File_Test"
		},
		"LnkSR_File_Server_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "CORP\\ADM-IN-DEVOPS"
		},
		"blobtrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2ca69f6a-bdaf-49aa-a407-7b487eed4bf5/resourceGroups/rak-workshop-arm/providers/Microsoft.Storage/storageAccounts/strgaccountdevadf"
		},
		"Linked-Self-Hosted-RunTime_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/05284a4d-7087-4dc1-ab12-8304163e3dec/resourcegroups/Cloud-BI-Testing/providers/Microsoft.DataFactory/factories/rak-warehouse-sih-df/integrationruntimes/Self-Hosted-IntegrationRunTime-44"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rak-warehouse-dev-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Lnk_Service_AzSQL_DB_connectionString": {
						"value": "[parameters('Lnk_Service_AzSQL_DB_connectionString')]"
					},
					"AKV_Test_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKV_Test_properties_typeProperties_baseUrl')]"
					},
					"blobtrigger_properties_typeProperties_scope": {
						"value": "[parameters('blobtrigger_properties_typeProperties_scope')]"
					},
					"Linked-Self-Hosted-RunTime_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('Linked-Self-Hosted-RunTime_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"LnkSR_File_Server_properties_typeProperties_host": {
						"value": "[parameters('LnkSR_File_Server_properties_typeProperties_host')]"
					},
					"LnkSR_File_Server_properties_typeProperties_userId": {
						"value": "[parameters('LnkSR_File_Server_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}